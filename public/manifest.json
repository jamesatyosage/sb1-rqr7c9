{
  "manifest_version": 3,
  "name": "Therapy Co-Pilot",
  "version": "1.0",
  "description": "AI-powered assistant for teletherapy sessions",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["assets/index.js"],
      "css": ["assets/index.css"]
    }
  ]
}